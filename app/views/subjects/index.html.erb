<div class="container">
    <table class="table table-hover">
      <thead class="thead-light">
        <tr>
          <th>Titel</th>
        </tr>
      </thead>
      <tbody>
        <% @subjects.each do |s| %>
          <tr>
            <td><%= link_to s.name, show_subjects_path(s.id) %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
</div>
